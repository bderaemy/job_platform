{
    "name": "xeer/jobs-demo-drupal",
    "description": "Demo jobs platform",
    "version": "v1.0-dev",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "BenoÃ®t de Raemy",
            "role": "Dev"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "project/adminimal_theme",
                "version": "1.3",
                "type": "drupal-theme",
                "dist": {
                    "url": "https://ftp.drupal.org/files/projects/adminimal_theme-8.x-1.3.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.25",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/captcha": "^1.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/ctools": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/hreflang": "^1.1",
        "drupal/pathauto": "^1.3",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.3",
        "drupal/simple_sitemap": "^3.0",
        "drupal/token": "^1.5",
        "drush/drush": "^9.0.0",
        "project/adminimal_theme": "^1.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0",
        "drupal/devel": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "exclude": [
                ".csslintrc",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc",
                ".eslintrc.json",
                ".gitattributes",
                ".htaccess",
                "robots.txt",
                "sites/default/default.settings.php",
                "sites/default/default.services.yml",
                "sites/development.services.yml",
                "sites/example.settings.local.php",
                "sites/example.sites.php",
                "web.config",
                "update.php"
            ],
            "includes": [
                "index.php"
            ],
            "omit-defaults": true
        },
        "patches": {}
    }
}
