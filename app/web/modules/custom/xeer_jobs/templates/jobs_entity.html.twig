{#
/**
 * @file jobs_entity.html.twig
 * Default theme implementation to present Job position data.
 *
 * This template is used when viewing Job position pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_jobs_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('jobs_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
